package main;
import processing.core.PApplet;
import processing.core.PImage;

public class Carro {
	
	private PApplet app;
	private PImage modelSFoto;
	int precio;
	int velocidad;
	int rines;
	
public Carro (PApplet app) {
	
	this.app=app;

	
}

public void cargar() {
	modelSFoto=app.loadImage("img/modelSFoto.png");
}

public void pintarTipoC() {
	app.image (modelSFoto,0,0);
}
}

