package main;
import processing.core.PApplet;
import processing.core.PImage;

public abstract class CarroTesla {
	
	
	int precio;
	PApplet app;
	PImage carroSFoto;
	
public CarroTesla() {
	this.carroSFoto=app.loadImage("img/modelSFoto.png");
	
}

public void pintar() {
	app.image(carroSFoto,0,0);
}
}
