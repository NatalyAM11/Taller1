package main;
import processing.core.PApplet;
import processing.core.PImage;

public class CarroTesla {
	
	
	int precio;
	int posX;
	int posY;
	PApplet app;
	PImage carroSFoto;
	
public CarroTesla(int posX, int posY, PApplet app) {
	this.app = app;
	this.posX=posX;
	this.posY=posY;
}

public void pintar() {
	app.image(carroSFoto,posX,posY);
}

public void poner() {
	carroSFoto = app.loadImage("img/pInfoX.png");
	
}


}
